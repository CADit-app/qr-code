{
  "name": "@cadit-app/qr-code",
  "version": "0.5.1",
  "description": "QR Code Generator for CADit - Generate customizable 3D QR codes",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "generate": "npx tsx cli.ts",
    "build:glb": "npx tsx cli.ts output.glb",
    "build:3mf": "npx tsx cli.ts output.3mf",
    "build:svg": "npx tsx cli.ts output.svg",
    "build:png": "npx tsx cli.ts output.png",
    "generate:svgs": "node scripts/generate-embedded-svgs.cjs"
  },
  "dependencies": {
    "@cadit-app/script-params": "0.4.1",
    "@cadit-app/svg-sampler": "^0.1.0",
    "@resvg/resvg-wasm": "^2.6.2",
    "@types/qrcode": "^1.5.5",
    "qrcode": "^1.5.4"
  },
  "peerDependencies": {
    "@cadit-app/manifold-3d": "^3.0.0"
  },
  "devDependencies": {
    "@cadit-app/manifold-3d": "^3.3.2",
    "@types/node": "^25.0.3",
    "@types/qrcode": "^1.5.5",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "cadit",
    "qr-code",
    "3d-printing",
    "parametric"
  ],
  "author": {
    "name": "CADit"
  },
  "license": "MIT",
  "files": [
    "dist/**/*",
    "src/**/*",
    "images/**/*",
    "cadit.json",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
